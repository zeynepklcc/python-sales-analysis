    "import matplotlib.pyplot as plt\n",
    "\n",
    "df = pd.read_csv(\"sales.csv\")\n",
    "\n",
    "df.groupby(\"Product\")[\"Total Revenue\"].sum().plot(kind=\"bar\")\n",
    "plt.title(\"Ürün Bazında Toplam Gelir\")\n",
    "plt.ylabel(\"Gelir\")\n",
    "plt.show()"
   ]
